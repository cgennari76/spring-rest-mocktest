pipeline {
  agent any
  
  stages {
    stage('Hello') {
      agent {
        docker { 
	         image 'chrisgennari/dry-run-cloud:jenkins-ansible'
		 args '--entrypoint='
	       }
      }
      steps {
        sh "sudo ansible-galaxy  --version"
	sh "sudo ansible-playbook --version"
      }
    }
  }
}
